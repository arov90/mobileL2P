@{
    ViewData["Title"] = "Hyperlinks";
}

<div class="mdl-cell mdl-cell--12-col">

    <h4>@ViewData["Title"]</h4>
    <hr />

    <table align="left">
        <tr>
            <td style="align-content:flex-end">
                <button class="mdl-button mdl-js-button mdl-button--fab mdl-button--mini-fab" title="Add Assignment">
                    <i class="material-icons">add</i>
                </button>
            </td>
            <td>
                <button class="mdl-button mdl-js-button mdl-button--fab mdl-button--mini-fab" title="Corrected Assignments">
                    <i class="material-icons">delete</i>
                </button>
            </td>
        </tr>
    </table>
</div>
<div class="mdl-cell mdl-cell--12-col">
    <table align="center" class="mdl-data-table mdl-js-data-table mdl-data-table--selectable">
        <thead>
            <tr>
                <th class="mdl-data-table__cell--non-numeric">Url</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td class="mdl-data-table__cell--non-numeric"><a href="https://www.cs.umd.edu/users/ben/goldenrules.html">8 Golden Rules of Design</a></td>
            </tr>
            <tr>
                <td class="mdl-data-table__cell--non-numeric"><a href="https://www.cs.umd.edu/users/ben/goldenrules.html">Low- vs. High-fidelity Prototyping Debate</a></td>
            </tr>
        </tbody>
    </table>
</div>


